{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/Okta.Sdk.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**", "docs/**" ],
          "src": "../"
		  }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "netcoreapp3.1"
      },
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Okta .NET SDK API Documentation",
      "_appFooter": "Copyright &copy; 2021 Okta, Inc.<br>Generated with <b>DocFx</b>.",
      "_appLogoPath": "images/okta-dev-logo.png",
      "_appFaviconPath": "images/favicon.ico",
      "_disableContribution": "true"
    },
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
